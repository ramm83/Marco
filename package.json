{
    "name": "fuse",
    "version": "10.0.0",
    "license": "https://themeforest.net/licenses/terms/regular",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --open",
        "start:mem": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng serve --open",
        "build": "ng build",
        "build:prod": "ng build --prod",
        "build:prod:mem": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "internet-dev": "ng build --outputPath=dist/SGL",
        "internet-qa": "ng build -c qa --outputPath=dist/SGL",
        "internet-prod": "ng build --prod -c production --outputPath=dist/SGL",
        "all-internet": "npm run-script internet-dev & npm run-script internet-qa & npm run-script internet-prod",
        "intranet-dev": "ng build --outputPath=dist/SGL --baseHref=/Aplicaciones/SGL/",
        "intranet-qa": "ng build -c qa --outputPath=dist/SGL --baseHref=/Aplicaciones/SGL/",
        "intranet-prod": "ng build --prod -c production --outputPath=dist/SGL --baseHref=/Aplicaciones/SGL/",
        "all-intranet": "npm run-script intranet-dev & npm run-script intranet-qa & npm run-script intranet-prod",
        "all": "npm run-script all-internet & npm run-script all-intranet",
        "bundle-report": "webpack-bundle-analyzer dist/stats.json"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "10.0.5",
        "@angular/cdk": "10.0.0",
        "@angular/common": "10.0.5",
        "@angular/compiler": "10.0.5",
        "@angular/core": "10.0.5",
        "@angular/flex-layout": "10.0.0-beta.32",
        "@angular/forms": "10.0.5",
        "@angular/material": "10.1.0",
        "@angular/material-moment-adapter": "10.1.0",
        "@angular/platform-browser": "10.0.5",
        "@angular/platform-browser-dynamic": "10.0.5",
        "@angular/router": "10.0.5",
        "@material-extended/mde": "3.0.3",
        "@ng-select/ng-select": "5.0.1",
        "@ngx-translate/core": "13.0.0",
        "@swimlane/ngx-datatable": "17.1.0",
        "@types/crypto-js": "3.1.47",
        "@types/prismjs": "1.16.1",
        "angular2-txt": "^0.1.1",
        "crypto-js": "^3.1.9-1",
        "file-saver": "^2.0.2",
        "lodash": "4.17.19",
        "moment": "2.27.0",
        "ngx-cookie-service": "3.1.2",
        "ngx-countdown": "10.0.0",
        "perfect-scrollbar": "1.5.0",
        "prismjs": "1.20.0",
        "rxjs": "6.6.0",
        "tslib": "2.0.0",
        "web-animations-js": "2.3.2",
        "xlsx": "^0.14.3",
        "zone.js": "0.10.3"
    },
    "devDependencies": {
        "@angular/cli": "10.0.4",
        "@angular/compiler-cli": "10.0.5",
        "@angular-devkit/build-angular": "0.1000.4",
        "@types/lodash": "4.14.149",
        "codelyzer": "6.0.0",
        "jasmine-core": "3.5.0",
        "jasmine-spec-reporter": "5.0.2",
        "karma": "5.1.0",
        "karma-chrome-launcher": "3.1.0",
        "karma-coverage-istanbul-reporter": "3.0.3",
        "karma-jasmine": "3.3.1",
        "karma-jasmine-html-reporter": "1.5.4",
        "protractor": "7.0.0",
        "ts-node": "8.3.0",
        "tslint": "6.1.2",
        "tslib": "2.0.0",
        "typescript": "3.9.7",
        "webpack-bundle-analyzer": "3.8.0"
    }
}
